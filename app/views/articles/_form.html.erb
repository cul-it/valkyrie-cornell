<%= simple_form_for @change_set do |f| %>
  <% (@change_set.class.fields - [:member_ids]).each do |field| %>
    <%= render_edit_field_partial(field, f: f) %>
  <% end %>
  <% if @change_set.append_id.present? %>
    <%= f.input :append_id, as: :hidden %>
  <% end %>
  <% if @change_set.respond_to?(:files) %>
    <%= f.input :files, as: :file, input_html: { multiple: true }%>
  <% end %>
  <%= f.button :submit %>
<% end %>
